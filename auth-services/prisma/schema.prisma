generator client {
  provider = "prisma-client-js"
  engineType    = "library"  // or "binary" â€” not "client"

}

datasource db {
  provider = "postgresql"

}

model User {
  id           String   @id @default(uuid())
  email        String   @unique
  passwordHash String
  role         String   @default("user")
  createdAt    DateTime @default(now())
}
